{
    "servers": {
        "noc-server": {
          "name": "NOC MCP Server",
          "description": "NOC MCP Server for development",
          "command": "~/.local/bin/uv",
          "args": ["--directory", "core", "run", "mcp_server.py", "--port", "8075"],
          "connection_type": "sse",
          "url": "http://localhost:8075/sse",
          "enabled": true,
          "timeout": 30.0,
          "ping_interval": 20.0,
          "ping_timeout": 3.0
        }
  }
}
